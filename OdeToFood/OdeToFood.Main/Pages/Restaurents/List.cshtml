@page
@model OdeToFood.Main.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<p>@Model.Message</p>

<table class="table">
    @foreach (var restaurant in Model.Restaurents)
    {
        <tr>
            <td>@restaurant.Id</td>
            <td>@restaurant.Name</td>
            <td>@restaurant.Location</td>
            <td>@restaurant.Cuisine</td>
        </tr>
    }
</table>